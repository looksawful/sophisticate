{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Sophisticate: Dev + Auto Browser",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev:auto",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Sophisticate: Dev + Chrome Debug",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "Sophisticate: Dev Server"
    }
  ]
}
